{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "looks",
  "title": "Looks",
  "description": "A snippet payload from looks.wtf",
  "icon": "command-icon.png",
  "author": "mmccoy",
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Search Looks.wtf",
      "description": "Search for and copy looks.wtf expressions to your clipboard",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.30.2",
    "js-yaml": "^4.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}
